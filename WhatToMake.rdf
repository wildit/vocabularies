    </owl:DatatypeProperty>
    


    <!-- &wtm;hasGlycemicIndex -->

    <owl:DatatypeProperty rdf:about="&wtm;hasGlycemicIndex">
        <dct:source>MÃ©thod Montignac , "The Glycemic Index Concept," 2014. [Online]. Available: http://www.montignac.com/en/the-glycemic-index-concept/. [Accessed Nov. 1 2018]</dct:source>
        <rdfs:label>has glycemic index</rdfs:label>
        <skos:definition>has a glycemic indexe value which measures carbsâ€™ from the perspective of their pure sugar/starch content in order to determine how they affect glycemia (blood sugar levels)  after meals</skos:definition>
        <skos:scopeNote>ingredient property</skos:scopeNote>
    </owl:DatatypeProperty>
    


    <!-- &wtm;serves -->

